{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "dev": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "dev:demo": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^typecheck", "^build"],
      "outputs": ["dist/**"]
    },
    "prettier": {},
    "eslint": {},
    "prettier:fix": {},
    "eslint:fix": {},
    "//#eslint:root": {},
    "//#eslint:root:fix": {},
    "typecheck": {
      "dependsOn": ["^typecheck"],
      "inputs": ["**/*.ts"]
    }
  }
}
