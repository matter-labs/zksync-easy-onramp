  "description": "SDK package for ZKsync easy on ramp",
  "keywords": [
    "zksync",
    "sdk",
    "on-ramp"
  ],
  "license": "(MIT OR Apache-2.0)",
  "author": "Matter Labs (https://matter-labs.io/)",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/zksync-easy-onramp.cjs",
  "module": "./dist/zksync-easy-onramp.esm.js",
